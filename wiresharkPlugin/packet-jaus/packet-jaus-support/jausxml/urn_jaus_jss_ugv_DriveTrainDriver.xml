<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<service_def name="DriveTrainDriver" id="urn:jaus:jss:ugv:DriveTrainDriver" version="1.0" xmlns="urn:jaus:jsidl:1.1" xmlns:ns2="urn:jaus:jsidl:plus">
    <description xml:space="preserve">The DrivetrainDriver provides the means to control transmissions</description>
    <assumptions xml:space="preserve">Messages may be delayed, lost or reordered.</assumptions>
    <references>
        <inherits_from name="management" id="urn:jaus:jss:core:Management" version="1.1"/>
    </references>
    <message_set>
        <input_set>
            <message_def name="SetTransmissionState" message_id="0509" is_command="true">
                <description xml:space="preserve">Sets desired transmission state</description>
                <header name="header">
                    <record name="HeaderRec" optional="false">
                        <fixed_field name="MessageID" optional="false" interpretation="Two byte field to hold message ID" field_type="unsigned short integer" field_units="one"/>
                    </record>
                </header>
                <body name="body">
                    <record name="transmissionStateRec" optional="false">
                        <fixed_field name="TransmissionState" optional="false" field_type="unsigned byte" field_units="one">
                            <value_set offset_to_lower_limit="false">
                                <value_enum enum_index="0" enum_const="DEFAULT"/>
                                <value_enum enum_index="1" enum_const="PARK"/>
                                <value_enum enum_index="2" enum_const="NEUTRAL"/>
                                <value_enum enum_index="3" enum_const="REVERSE"/>
                                <value_enum enum_index="4" enum_const="DRIVE"/>
                                <value_enum enum_index="5" enum_const="OVERDRIVE"/>
                                <value_enum enum_index="6" enum_const="L1"/>
                                <value_enum enum_index="7" enum_const="L2"/>
                                <value_enum enum_index="8" enum_const="L3"/>
                                <value_enum enum_index="9" enum_const="L4"/>
                                <value_enum enum_index="10" enum_const="L5"/>
                                <value_enum enum_index="11" enum_const="L6"/>
                                <value_enum enum_index="12" enum_const="L7"/>
                                <value_enum enum_index="13" enum_const="L8"/>
                                <value_enum enum_index="14" enum_const="L9"/>
                                <value_enum enum_index="15" enum_const="L10"/>
                            </value_set>
                        </fixed_field>
                    </record>
                </body>
                <footer name="footer"/>
            </message_def>
            <message_def name="QueryTransmissionState" message_id="2509" is_command="false">
                <description xml:space="preserve">Requests current transmission state</description>
                <header name="header">
                    <record name="HeaderRec" optional="false">
                        <fixed_field name="MessageID" optional="false" interpretation="Two byte field to hold message ID" field_type="unsigned short integer" field_units="one"/>
                    </record>
                </header>
                <body name="body"/>
                <footer name="footer"/>
            </message_def>
            <message_def name="SetTransferCaseState" message_id="0510" is_command="true">
                <description xml:space="preserve">Sets desired transfer case state for Front Wheel Drive or All Wheel Drive vehicles</description>
                <header name="header">
                    <record name="HeaderRec" optional="false">
                        <fixed_field name="MessageID" optional="false" interpretation="Two byte field to hold message ID" field_type="unsigned short integer" field_units="one"/>
                    </record>
                </header>
                <body name="body">
                    <record name="transferCaseState" optional="false">
                        <fixed_field name="TransferCaseState" optional="false" field_type="unsigned byte" field_units="one">
                            <value_set offset_to_lower_limit="false">
                                <value_enum enum_index="0" enum_const="DEFAULT"/>
                                <value_enum enum_index="1" enum_const="FWD"/>
                                <value_enum enum_index="2" enum_const="AUTO_4WD"/>
                                <value_enum enum_index="3" enum_const="MANUAL_LOW_4WD"/>
                                <value_enum enum_index="4" enum_const="MANUAL_HIGH_4WD"/>
                                <value_enum enum_index="5" enum_const="LOW_AWD"/>
                                <value_enum enum_index="6" enum_const="HIGH_AWD"/>
                            </value_set>
                        </fixed_field>
                    </record>
                </body>
                <footer name="footer"/>
            </message_def>
            <message_def name="QueryTransferCaseState" message_id="2510" is_command="false">
                <description xml:space="preserve">Requests current transfer case state</description>
                <header name="header">
                    <record name="HeaderRec" optional="false">
                        <fixed_field name="MessageID" optional="false" interpretation="Two byte field to hold message ID" field_type="unsigned short integer" field_units="one"/>
                    </record>
                </header>
                <body name="body"/>
                <footer name="footer"/>
            </message_def>
            <message_def name="QueryTransmissionCapabilities" message_id="2511" is_command="false">
                <description xml:space="preserve">Queries for a bitfield of valid gears and transfer case settings</description>
                <header name="header">
                    <record name="HeaderRec" optional="false">
                        <fixed_field name="MessageID" optional="false" interpretation="Two byte field to hold message ID" field_type="unsigned short integer" field_units="one"/>
                    </record>
                </header>
                <body name="body"/>
                <footer name="footer"/>
            </message_def>
        </input_set>
        <output_set>
            <message_def name="ReportTransmissionState" message_id="4509" is_command="false">
                <description xml:space="preserve">Returns current transmission state</description>
                <header name="header">
                    <record name="HeaderRec" optional="false">
                        <fixed_field name="MessageID" optional="false" interpretation="Two byte field to hold message ID" field_type="unsigned short integer" field_units="one"/>
                    </record>
                </header>
                <body name="body">
                    <record name="reportTransmissionStateRec" optional="false">
                        <fixed_field name="RequestedTransmissionState" optional="false" field_type="unsigned byte" field_units="one">
                            <value_set offset_to_lower_limit="false">
                                <value_enum enum_index="0" enum_const="DEFAULT"/>
                                <value_enum enum_index="1" enum_const="PARK"/>
                                <value_enum enum_index="2" enum_const="NEUTRAL"/>
                                <value_enum enum_index="3" enum_const="REVERSE"/>
                                <value_enum enum_index="4" enum_const="DRIVE"/>
                                <value_enum enum_index="5" enum_const="OVERDRIVE"/>
                                <value_enum enum_index="6" enum_const="L1"/>
                                <value_enum enum_index="7" enum_const="L2"/>
                                <value_enum enum_index="8" enum_const="L3"/>
                                <value_enum enum_index="9" enum_const="L4"/>
                                <value_enum enum_index="10" enum_const="L5"/>
                                <value_enum enum_index="11" enum_const="L6"/>
                                <value_enum enum_index="12" enum_const="L7"/>
                                <value_enum enum_index="13" enum_const="L8"/>
                                <value_enum enum_index="14" enum_const="L9"/>
                                <value_enum enum_index="15" enum_const="L10"/>
                            </value_set>
                        </fixed_field>
                        <fixed_field name="ActualTransmissionState" optional="false" field_type="unsigned byte" field_units="one">
                            <value_set offset_to_lower_limit="false">
                                <value_enum enum_index="0" enum_const="DEFAULT"/>
                                <value_enum enum_index="1" enum_const="PARK"/>
                                <value_enum enum_index="2" enum_const="NEUTRAL"/>
                                <value_enum enum_index="3" enum_const="REVERSE"/>
                                <value_enum enum_index="4" enum_const="DRIVE"/>
                                <value_enum enum_index="5" enum_const="OVERDRIVE"/>
                                <value_enum enum_index="6" enum_const="L1"/>
                                <value_enum enum_index="7" enum_const="L2"/>
                                <value_enum enum_index="8" enum_const="L3"/>
                                <value_enum enum_index="9" enum_const="L4"/>
                                <value_enum enum_index="10" enum_const="L5"/>
                                <value_enum enum_index="11" enum_const="L6"/>
                                <value_enum enum_index="12" enum_const="L7"/>
                                <value_enum enum_index="13" enum_const="L8"/>
                                <value_enum enum_index="14" enum_const="L9"/>
                                <value_enum enum_index="15" enum_const="L10"/>
                            </value_set>
                        </fixed_field>
                    </record>
                </body>
                <footer name="footer"/>
            </message_def>
            <message_def name="ReportTransferCaseState" message_id="4510" is_command="false">
                <description xml:space="preserve">Returns current transfer case state for Front Wheel Drive or All Wheel Drive vehicles</description>
                <header name="header">
                    <record name="HeaderRec" optional="false">
                        <fixed_field name="MessageID" optional="false" interpretation="Two byte field to hold message ID" field_type="unsigned short integer" field_units="one"/>
                    </record>
                </header>
                <body name="body">
                    <record name="transferCaseState" optional="false">
                        <fixed_field name="TransferCaseState" optional="false" field_type="unsigned byte" field_units="one">
                            <value_set offset_to_lower_limit="false">
                                <value_enum enum_index="0" enum_const="DEFAULT"/>
                                <value_enum enum_index="1" enum_const="FWD"/>
                                <value_enum enum_index="2" enum_const="AUTO_4WD"/>
                                <value_enum enum_index="3" enum_const="MANUAL_LOW_4WD"/>
                                <value_enum enum_index="4" enum_const="MANUAL_HIGH_4WD"/>
                                <value_enum enum_index="5" enum_const="LOW_AWD"/>
                                <value_enum enum_index="6" enum_const="HIGH_AWD"/>
                            </value_set>
                        </fixed_field>
                    </record>
                </body>
                <footer name="footer"/>
            </message_def>
            <message_def name="ReportTransmissionCapabilities" message_id="4511" is_command="false">
                <description xml:space="preserve">Returns a bitfield of valid gears and transfer case settings</description>
                <header name="header">
                    <record name="HeaderRec" optional="false">
                        <fixed_field name="MessageID" optional="false" interpretation="Two byte field to hold message ID" field_type="unsigned short integer" field_units="one"/>
                    </record>
                </header>
                <body name="body">
                    <record name="transmissionCapabilities" optional="false">
                        <bit_field name="transmissionBF" optional="false" interpretation="When the bit is set high, the vehicle supports the specified mode.  Otherwise, it is not supported." field_type_unsigned="unsigned short integer">
                            <sub_field name="PARK">
                                <bit_range from_index="0" to_index="0"/>
                                <value_set offset_to_lower_limit="false">
                                    <value_enum enum_index="0" enum_const="Unsupported"/>
                                    <value_enum enum_index="1" enum_const="Supported"/>
                                </value_set>
                            </sub_field>
                            <sub_field name="NEUTRAL">
                                <bit_range from_index="1" to_index="1"/>
                                <value_set offset_to_lower_limit="false">
                                    <value_enum enum_index="0" enum_const="Unsupported"/>
                                    <value_enum enum_index="1" enum_const="Supported"/>
                                </value_set>
                            </sub_field>
                            <sub_field name="REVERSE">
                                <bit_range from_index="2" to_index="2"/>
                                <value_set offset_to_lower_limit="false">
                                    <value_enum enum_index="0" enum_const="Unsupported"/>
                                    <value_enum enum_index="1" enum_const="Supported"/>
                                </value_set>
                            </sub_field>
                            <sub_field name="DRIVE">
                                <bit_range from_index="3" to_index="3"/>
                                <value_set offset_to_lower_limit="false">
                                    <value_enum enum_index="0" enum_const="Unsupported"/>
                                    <value_enum enum_index="1" enum_const="Supported"/>
                                </value_set>
                            </sub_field>
                            <sub_field name="OVERDRIVE">
                                <bit_range from_index="4" to_index="4"/>
                                <value_set offset_to_lower_limit="false">
                                    <value_enum enum_index="0" enum_const="Unsupported"/>
                                    <value_enum enum_index="1" enum_const="Supported"/>
                                </value_set>
                            </sub_field>
                            <sub_field name="L1">
                                <bit_range from_index="5" to_index="5"/>
                                <value_set offset_to_lower_limit="false">
                                    <value_enum enum_index="0" enum_const="Unsupported"/>
                                    <value_enum enum_index="1" enum_const="Supported"/>
                                </value_set>
                            </sub_field>
                            <sub_field name="L2">
                                <bit_range from_index="6" to_index="6"/>
                                <value_set offset_to_lower_limit="false">
                                    <value_enum enum_index="0" enum_const="Unsupported"/>
                                    <value_enum enum_index="1" enum_const="Supported"/>
                                </value_set>
                            </sub_field>
                            <sub_field name="L3">
                                <bit_range from_index="7" to_index="7"/>
                                <value_set offset_to_lower_limit="false">
                                    <value_enum enum_index="0" enum_const="Unsupported"/>
                                    <value_enum enum_index="1" enum_const="Supported"/>
                                </value_set>
                            </sub_field>
                            <sub_field name="L4">
                                <bit_range from_index="8" to_index="8"/>
                                <value_set offset_to_lower_limit="false">
                                    <value_enum enum_index="0" enum_const="Unsupported"/>
                                    <value_enum enum_index="1" enum_const="Supported"/>
                                </value_set>
                            </sub_field>
                            <sub_field name="L5">
                                <bit_range from_index="9" to_index="9"/>
                                <value_set offset_to_lower_limit="false">
                                    <value_enum enum_index="0" enum_const="Unsupported"/>
                                    <value_enum enum_index="1" enum_const="Supported"/>
                                </value_set>
                            </sub_field>
                            <sub_field name="L6">
                                <bit_range from_index="10" to_index="10"/>
                                <value_set offset_to_lower_limit="false">
                                    <value_enum enum_index="0" enum_const="Unsupported"/>
                                    <value_enum enum_index="1" enum_const="Supported"/>
                                </value_set>
                            </sub_field>
                            <sub_field name="L7">
                                <bit_range from_index="11" to_index="11"/>
                                <value_set offset_to_lower_limit="false">
                                    <value_enum enum_index="0" enum_const="Unsupported"/>
                                    <value_enum enum_index="1" enum_const="Supported"/>
                                </value_set>
                            </sub_field>
                            <sub_field name="L8">
                                <bit_range from_index="12" to_index="12"/>
                                <value_set offset_to_lower_limit="false">
                                    <value_enum enum_index="0" enum_const="Unsupported"/>
                                    <value_enum enum_index="1" enum_const="Supported"/>
                                </value_set>
                            </sub_field>
                            <sub_field name="L9">
                                <bit_range from_index="13" to_index="13"/>
                                <value_set offset_to_lower_limit="false">
                                    <value_enum enum_index="0" enum_const="Unsupported"/>
                                    <value_enum enum_index="1" enum_const="Supported"/>
                                </value_set>
                            </sub_field>
                            <sub_field name="L10">
                                <bit_range from_index="14" to_index="14"/>
                                <value_set offset_to_lower_limit="false">
                                    <value_enum enum_index="0" enum_const="Unsupported"/>
                                    <value_enum enum_index="1" enum_const="Supported"/>
                                </value_set>
                            </sub_field>
                            <sub_field name="Reserved">
                                <bit_range from_index="15" to_index="15"/>
                                <value_set offset_to_lower_limit="false">
                                    <value_enum enum_index="0" enum_const="Unsupported"/>
                                    <value_enum enum_index="1" enum_const="Supported"/>
                                </value_set>
                            </sub_field>
                        </bit_field>
                        <bit_field name="transferCaseBF" optional="false" interpretation="When the bit is set high, the vehicle supports the specified mode. Otherwise, it is not supported." field_type_unsigned="unsigned byte">
                            <sub_field name="FWD">
                                <bit_range from_index="0" to_index="0"/>
                                <value_set offset_to_lower_limit="false">
                                    <value_enum enum_index="0" enum_const="Unsupported"/>
                                    <value_enum enum_index="1" enum_const="Supported"/>
                                </value_set>
                            </sub_field>
                            <sub_field name="AUTO_4WD">
                                <bit_range from_index="1" to_index="1"/>
                                <value_set offset_to_lower_limit="false">
                                    <value_enum enum_index="0" enum_const="Unsupported"/>
                                    <value_enum enum_index="1" enum_const="Supported"/>
                                </value_set>
                            </sub_field>
                            <sub_field name="MANUAL_LOW_4WD">
                                <bit_range from_index="2" to_index="2"/>
                                <value_set offset_to_lower_limit="false">
                                    <value_enum enum_index="0" enum_const="Unsupported"/>
                                    <value_enum enum_index="1" enum_const="Supported"/>
                                </value_set>
                            </sub_field>
                            <sub_field name="MANUAL_HIGH_4WD">
                                <bit_range from_index="3" to_index="3"/>
                                <value_set offset_to_lower_limit="false">
                                    <value_enum enum_index="0" enum_const="Unsupported"/>
                                    <value_enum enum_index="1" enum_const="Supported"/>
                                </value_set>
                            </sub_field>
                            <sub_field name="LOW_AWD">
                                <bit_range from_index="4" to_index="4"/>
                                <value_set offset_to_lower_limit="false">
                                    <value_enum enum_index="0" enum_const="Unsupported"/>
                                    <value_enum enum_index="1" enum_const="Supported"/>
                                </value_set>
                            </sub_field>
                            <sub_field name="HIGH_AWD">
                                <bit_range from_index="5" to_index="5"/>
                                <value_set offset_to_lower_limit="false">
                                    <value_enum enum_index="0" enum_const="Unsupported"/>
                                    <value_enum enum_index="1" enum_const="Supported"/>
                                </value_set>
                            </sub_field>
                            <sub_field name="Reserved_For_Future_Use">
                                <bit_range from_index="6" to_index="6"/>
                                <value_set offset_to_lower_limit="false">
                                    <value_enum enum_index="0" enum_const="Unsupported"/>
                                    <value_enum enum_index="1" enum_const="Supported"/>
                                </value_set>
                            </sub_field>
                            <sub_field name="Reserved_For_User_Defined_Use">
                                <bit_range from_index="7" to_index="7"/>
                                <value_set offset_to_lower_limit="false">
                                    <value_enum enum_index="0" enum_const="Unsupported"/>
                                    <value_enum enum_index="1" enum_const="Supported"/>
                                </value_set>
                            </sub_field>
                        </bit_field>
                    </record>
                </body>
                <footer name="footer"/>
            </message_def>
        </output_set>
    </message_set>
    <internal_events_set/>
    <protocol_behavior>
        <start state_machine_name="management.accessControl.events.transport.ReceiveFSM" state_name="Receiving.Ready.NotControlled"/>
        <start state_machine_name="management.accessControl.events.transport.SendFSM" state_name="Sending"/>
        <state_machine name="management.accessControl.events.transport.ReceiveFSM">
            <state name="Receiving" initial_state="Ready">
                <state name="Ready" initial_state="NotControlled">
                    <state name="NotControlled" initial_state="Available">
                        <state name="Available" initial_state="Standby">
                            <state name="Standby"/>
                        </state>
                        <state name="NotAvailable" initial_state="Init">
                            <state name="Init"/>
                            <state name="Failure"/>
                            <state name="Shutdown"/>
                            <state name="Emergency"/>
                        </state>
                        <default_state>
                            <transition name="management.accessControl.events.transport.Receive">
                                <parameter type="QueryTransmissionState" value="msg" interpretation="enveloped query                                                       transmission state message"/>
                                <parameter type="Receive.Body.ReceiveRec" value="transportData" interpretation="transport data"/>
                                <internal/>
                                <action name="sendReportTransmissionState" interpretation="Send a Report                                                      Transmission State message">
                                    <argument value="msg"/>
                                    <argument value="transportData"/>
                                </action>
                            </transition>
                            <transition name="management.accessControl.events.transport.Receive">
                                <parameter type="QueryTransferCaseState" value="msg" interpretation="enveloped query                                                       transfer case state message"/>
                                <parameter type="Receive.Body.ReceiveRec" value="transportData" interpretation="transport data"/>
                                <internal/>
                                <action name="sendReportTransferCaseState" interpretation="Send a Report                                                      Transfer Case State message">
                                    <argument value="msg"/>
                                    <argument value="transportData"/>
                                </action>
                            </transition>
                            <transition name="management.accessControl.events.transport.Receive">
                                <parameter type="QueryTransmissionCapabilities" value="msg" interpretation="enveloped query                                                       transmission capabililities message"/>
                                <parameter type="Receive.Body.ReceiveRec" value="transportData" interpretation="transport data"/>
                                <internal/>
                                <action name="sendReportTransmissionCapabilities" interpretation="Send a Report                                                      Transmission Capabilities message">
                                    <argument value="msg"/>
                                    <argument value="transportData"/>
                                </action>
                            </transition>
                        </default_state>
                    </state>
                    <state name="Controlled" initial_state="Available">
                        <state name="Available" initial_state="Standby">
                            <state name="Standby"/>
                            <state name="Ready">
                                <exit>
                                    <action name="shiftToPark" interpretation="When leaving the Ready state,                                                       the transmission should switch to Park once the vehicle has safely stopped."/>
                                </exit>
                                <transition name="management.accessControl.events.transport.Receive">
                                    <parameter type="SetTransmissionState" value="msg" interpretation="enveloped set transmission state message"/>
                                    <parameter type="Receive.Body.ReceiveRec" value="transportData" interpretation="transport data"/>
                                    <guard condition="isControllingClient( transportData ) &amp;&amp; isSupported( msg )" interpretation="True if the message that triggered the transition is received from the                                                       client that is in control of this service AND the specified transmission mode is supported by                                                      the service."/>
                                    <internal/>
                                    <action name="setTransmissionState" interpretation="Set the specified transmission state.">
                                        <argument value="msg"/>
                                    </action>
                                </transition>
                            </state>
                        </state>
                        <state name="NotAvailable" initial_state="Emergency">
                            <state name="Emergency"/>
                        </state>
                        <default_state>
                            <transition name="management.accessControl.events.transport.Receive">
                                <parameter type="SetTransmissionState" value="msg" interpretation="enveloped set transmission state message"/>
                                <parameter type="Receive.Body.ReceiveRec" value="transportData" interpretation="transport data"/>
                                <guard condition="isControllingClient( transportData ) &amp;&amp; isPark( msg )" interpretation="True if the message that triggered the transition is received from the                                                    client that is in control of this service AND the message is a request to shift to the 'Park'                                                   state.  If 'Park' is not supported, this condition is considered true for any transmission                              state that would prevent or inhibit motion."/>
                                <internal/>
                                <action name="setTransmissionState" interpretation="Set the specified transmission state.">
                                    <argument value="msg"/>
                                </action>
                            </transition>
                            <transition name="management.accessControl.events.transport.Receive">
                                <parameter type="SetTransferCaseState" value="msg" interpretation="enveloped set transfer case state message"/>
                                <parameter type="Receive.Body.ReceiveRec" value="transportData" interpretation="transport data"/>
                                <guard condition="isControllingClient( transportData ) &amp;&amp; isSupported( msg )" interpretation="True if the message that triggered the transition is received from the                                                    client that is in control of this service AND the specified transmission mode is supported by                                                  the service."/>
                                <internal/>
                                <action name="setTransferCaseState" interpretation="Set the specified transfer case state.">
                                    <argument value="msg"/>
                                </action>
                            </transition>
                            <transition name="management.accessControl.events.transport.Receive">
                                <parameter type="QueryTransmissionState" value="msg" interpretation="enveloped query transmission state message"/>
                                <parameter type="Receive.Body.ReceiveRec" value="transportData" interpretation="transport data"/>
                                <internal/>
                                <action name="sendReportTransmissionState" interpretation="Send a Report Transmission State message">
                                    <argument value="msg"/>
                                    <argument value="transportData"/>
                                </action>
                            </transition>
                            <transition name="management.accessControl.events.transport.Receive">
                                <parameter type="QueryTransferCaseState" value="msg" interpretation="enveloped query transfer case state message"/>
                                <parameter type="Receive.Body.ReceiveRec" value="transportData" interpretation="transport data"/>
                                <internal/>
                                <action name="sendReportTransferCaseState" interpretation="Send a Report Transfer Case State message">
                                    <argument value="msg"/>
                                    <argument value="transportData"/>
                                </action>
                            </transition>
                            <transition name="management.accessControl.events.transport.Receive">
                                <parameter type="QueryTransmissionCapabilities" value="msg" interpretation="enveloped query transmission capabililities message"/>
                                <parameter type="Receive.Body.ReceiveRec" value="transportData" interpretation="transport data"/>
                                <internal/>
                                <action name="sendReportTransmissionCapabilities" interpretation="Send a Report Transmission Capabilities message">
                                    <argument value="msg"/>
                                    <argument value="transportData"/>
                                </action>
                            </transition>
                        </default_state>
                    </state>
                    <default_state/>
                </state>
            </state>
        </state_machine>
        <state_machine name="management.accessControl.events.transport.SendFSM">
            <state name="Sending"/>
        </state_machine>
    </protocol_behavior>
</service_def>
